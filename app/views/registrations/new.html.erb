<h1>Sign Up</h1>
<%= form_with model: @user, url: sign_up_path do |form| %>
  <%= render "shared/form_errors", form: form %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Sign Up" %>
  </div>
<% end %>